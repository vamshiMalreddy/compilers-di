foo(){
    "foo"
}

bar(){
    "bar"
}

show(message){ message }

noise(){ "test" }

if_else(input){
    if (foo() in input)
        "Contains foo; "
    else
        "Does not contain foo; "
}

show(if_else(foo()))
show(if_else(bar()))
show(if_else(foo()+bar()))
show(if_else(noise()))
show(if_else(foo()+noise()+bar()))
